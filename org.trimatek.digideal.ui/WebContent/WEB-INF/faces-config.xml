<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<managed-bean>
		<managed-bean-name>contractView</managed-bean-name>
		<managed-bean-class>org.trimatek.digideal.ui.beans.ContractView</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>supportView</managed-bean-name>
		<managed-bean-class>org.trimatek.digideal.ui.beans.SupportView</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<application>
		<locale-config>
			<default-locale>es</default-locale>
			<supported-locale>es</supported-locale>
			<supported-locale>en</supported-locale>
			<supported-locale>pt</supported-locale>
		</locale-config>
		<resource-bundle>
			<base-name>org.trimatek.digideal.ui.messages</base-name>
			<var>msg</var>
		</resource-bundle>
	</application>
	<navigation-rule>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>result</from-outcome>
			<to-view-id>/result.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/result.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/support.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>send</from-outcome>
			<to-view-id>/thanks.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/thanks.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>support</from-outcome>
			<to-view-id>/support.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
</faces-config>
